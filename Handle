/**
 * 
 */

/**
 * 
 */
public class Handle {
    
    private int start;
    private int length;
    
    public Handle(int startPostion, int size) {
        
        this.start = startPostion;
        this.length = size;
        
    }
    
    public int getStart() {
        return this.start;
    }
    
    public int getLength() {
        return this.length;
    }

}
